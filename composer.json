{
    "name": "benefits-me/php-api-auth",
    "description": "Package to authenticate against Benefits.me API",
    "type": "library",
    "license": "GPL-3.0",
    "require": {
        "php": "^8.1",
        "laravel/framework": "^v11.0.0"
    },
    "autoload": {
        "psr-4": {
            "BenefitsMe\\ApiAuth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BenefitsMe\\ApiAuth\\Provider\\AuthServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "pestphp/pest": "^3.5",
        "orchestra/testbench": "^9.6"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}

{
    "name": "benefits-me/php-api-auth",
    "description": "Package to authenticate against Benefits.me API",
    "type": "library",
    "license": "GPL-3.0",
    "require": {
        "php": "^8.0",
        "illuminate/support": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "BenefitsMe\\ApiAuth\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BenefitsMe\\ApiAuth\\AuthServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
